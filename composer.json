{
    "name": "nfourtythree/entriessubset",
    "description": "Craft field type plugin that extends the core Entries field type to give extra settings with ability to restrict by entry type",
    "type": "craft-plugin",
    "version": "1.0.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "entries subset",
        "entriessubset",
        "craft3",
        "entry types",
        "entrytype"
    ],
    "support": {
        "docs": "https://github.com/nfourtythree/craft3-entriessubset/blob/master/README.md",
        "issues": "https://github.com/nfourtythree/craft3-entriessubset/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nathaniel Hammond (nfourtythree)",
            "homepage": "http://n43.me"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
          "nfourtythree\\entriessubset\\": "src/"
        }
    },
    "extra": {
        "name": "Entries Subset",
        "handle": "entriessubset",
        "schemaVersion": "1.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/nfourtythree/craft3-entriessubset/master/CHANGELOG.md",
        "class": "nfourtythree\\entriessubset\\EntriesSubset"
    }
}
